<h1>Log In</h1>

<%= form_tag session_create_path do %>
	<div class="form-group">
		<%= label_tag :email %>
		<br>
		<%= text_field_tag :email, nil, class: "form-control", style: "width: 33%" %>
	</div>
	
	<div class="form-group">
		<%= label_tag :password %>
		<br>
		<%= password_field_tag :password, nil, class: "form-control", style: "width: 33%" %>
	</div>
	
	<%= submit_tag "Go", class: "btn btn-success" %>
<% end %>
